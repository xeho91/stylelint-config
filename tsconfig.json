{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",

    "outDir": "./lib/",

    "declaration": false,

    "types": ["node", "stylelint"],

    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules/"]
}
